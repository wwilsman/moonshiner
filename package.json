{
  "name": "moonshiner",
  "description": "High-proof testing",
  "version": "0.15.2",
  "license": "MIT",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "lib/harness.js",
  "exports": {
    ".": {
      "node": "./lib/harness.node.js",
      "default": "./lib/harness.js"
    },
    "./reporters": {
      "default": "./lib/reporter/index.js"
    },
    "./reporter/*": {
      "default": "./lib/reporter/*.js"
    },
    "./plugins": {
      "node": "./lib/plugin/index.node.js",
      "default": "./lib/plugin/index.js"
    },
    "./plugin/*": {
      "default": "./lib/plugin/*.js"
    }
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "extract-zip": "^2.0.1",
    "glob": "^10.3.12",
    "mime": "^4.0.1",
    "rimraf": "^5.0.1",
    "tar-fs": "^3.0.3",
    "unbzip2-stream": "^1.4.3",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1"
  }
}
